<%= form_with(model: @grade, local: true) do |f| %>
  <%= render 'shared/error_messages', object: @grade %>  

  <%= f.label :"Score" %>
  <%= f.text_field :score, class: 'form-control' %>
  <%= f.label :"Remarks" %>
  <%= f.text_area :remarks, class: 'form-control' %>
  <%= f.hidden_field :student_id %>
  <%= f.hidden_field :team_id %>


    <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>

